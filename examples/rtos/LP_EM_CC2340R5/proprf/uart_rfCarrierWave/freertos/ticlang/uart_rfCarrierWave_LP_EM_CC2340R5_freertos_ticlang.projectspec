<?xml version="1.0" encoding="UTF-8"?>
<projectSpec>
    <applicability>
        <when>
            <context
                deviceFamily="ARM"
                deviceId="Cortex M.CC2340R5"
            />
        </when>
    </applicability>

    <project
        title="UART RF Carrier Wave"
        name="uart_rfCarrierWave_LP_EM_CC2340R5_freertos_ticlang"
        configurations="Debug"
        toolChain="TICLANG"
        cgtVersion="TICLANG_3.2.0.LTS"
        connection="TIXDS110_Connection.xml"
        device="Cortex M.CC2340R5"
        ignoreDefaultDeviceSettings="true"
        ignoreDefaultCCSSettings="true"
        products="com.ti.SIMPLELINK_LOWPOWER_F3_SDK:7.40.00.64;sysconfig:1.18.1"
        compilerBuildOptions="
            -I${PROJECT_ROOT}
            -I${PROJECT_ROOT}/${ConfigName}
            -I${COM_TI_SIMPLELINK_LOWPOWER_F3_SDK_INSTALL_DIR}/source
            -I${COM_TI_SIMPLELINK_LOWPOWER_F3_SDK_INSTALL_DIR}/kernel/freertos
            -I${COM_TI_SIMPLELINK_LOWPOWER_F3_SDK_INSTALL_DIR}/source/ti/posix/ticlang
            -I${COM_TI_SIMPLELINK_LOWPOWER_F3_SDK_INSTALL_DIR}/source/third_party/freertos/include
            -I${COM_TI_SIMPLELINK_LOWPOWER_F3_SDK_INSTALL_DIR}/source/third_party/freertos/portable/GCC/ARM_CM0
            -gdwarf-3
            -mcpu=cortex-m0plus
            -march=thumbv6m
            -mfloat-abi=soft
            -mthumb
        "
        linkerBuildOptions="
            -L${COM_TI_SIMPLELINK_LOWPOWER_F3_SDK_INSTALL_DIR}/source
            -L${PROJECT_BUILD_DIR}/syscfg
            -lti_utils_build_linker.cmd.genlibs
            -Wl,--rom_model
            -Wl,--warn_sections
            -L${CG_TOOL_ROOT}/lib
            -llibc.a
        "
        enableSysConfigTool="true"
        sysConfigBuildOptions="
            --compiler ticlang
        "
        description="An example that uses the RF driver to generate a carrier wave">

        <property name="isHybrid" value="true"/>
        <file path="../../uart_rfCarrierWave.c" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../../README.md" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../../freertos/uart_rfCarrierWave.syscfg" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../../main_freertos.c" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../../freertos/ticlang/cc23x0r5_freertos.cmd" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../../setup/rcl_settings_ble_generic.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="setup">
        </file>
        <file path="../../setup/rcl_settings_ble_generic.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="setup">
        </file>
        <file path="../../setup/rcl_settings_msk_250_kbps_fec.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="setup">
        </file>
        <file path="../../setup/rcl_settings_msk_250_kbps_fec.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="setup">
        </file>
        <file path="../../setup/rcl_settings_msk_250_kbps.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="setup">
        </file>
        <file path="../../setup/rcl_settings_msk_250_kbps.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="setup">
        </file>
    </project>
</projectSpec>